<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pinet.rest.mapper.ActivityMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.pinet.rest.entity.Activity">
        <result column="id" property="id" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="name" property="name" />
        <result column="code" property="code" />
        <result column="is_new_user" property="isNewUser" />
        <result column="trigger" property="trigger" />
        <result column="target" property="target" />
        <result column="plans" property="plans" />
        <result column="publish_times" property="publishTimes" />
        <result column="admin_id" property="adminId" />
        <result column="is_deleted" property="isDeleted" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        create_time,
        update_time,
        name, code, is_new_user, trigger, target, plans, publish_times, admin_id, is_deleted
    </sql>

</mapper>
